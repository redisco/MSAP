<?xml version='1.0' encoding='UTF-8'?><Schema>    <Model Name='Подготовка начальных условий' Работает="да">        <Serial Forward="yes">            <CreateVariable VariableName="te2" DefaultValue='95'/>            <CreateVariable VariableName="te10" DefaultValue='9'/>            <CreateVariable VariableName="te4" DefaultValue='70'/>            <CreateVariable VariableName="Grazb" DefaultValue='1'/>	            <CreateVariable VariableName="Gmax" DefaultValue='10'/>            <CreateVariable VariableName="G8divGmax" DefaultValue='0.5'/>            <CreateVariable VariableName="SeasonSW" DefaultValue='1'/>	            <CreateVariable VariableName="a2" DefaultValue='60'/>			            <CreateVariable VariableName="G8" DefaultValue='1'/>            <CreateVariable VariableName="G4" DefaultValue='1'/>            <CreateVariable VariableName="G2" DefaultValue='1'/>            <CreateVariable VariableName="G10" DefaultValue='1'/>            <CreateVariable VariableName="G7" DefaultValue='1'/>            <CreateVariable VariableName="G8s" DefaultValue='1'/>            <CreateVariable VariableName="G5" DefaultValue='1'/>            <CreateVariable VariableName="te8" DefaultValue='1'/>            <CreateVariable VariableName="te5" DefaultValue='1'/>            <CreateVariable VariableName="te6" DefaultValue='1'/>            <CreateVariable VariableName="te3" DefaultValue='1'/>            <CreateVariable VariableName="te9" DefaultValue='1'/>            <CreateVariable VariableName="te11" DefaultValue='1'/>            <CreateVariable VariableName="te7" DefaultValue='1'/>            <CreateVariable VariableName="dp1" DefaultValue='3'/>	    </Serial>    </Model>	<Model Name='Нормирование сигнала о положении клапана на каждом цыкле' Работает="да">        <Serial Forward="yes">			<LoadVariable VariableName="a2" DefaultValue='60'/>				<Div><K>100</K></Div>			<SaveVariable VariableName="a2" DefaultValue='60'/>		    </Serial>    </Model>    <Model Name='G8' Работает="да">        <Serial Forward="yes">            <MultLinks Exp = "*">                <A><LoadVariable VariableName="Gmax" DefaultValue ='10'/></A>                <B><LoadVariable VariableName="G8divGmax" DefaultValue ='0.5'/></B>            </MultLinks>            <SaveVariable VariableName="G8" DefaultValue='1'/>	</Serial>    </Model>    <Model Name='G10' Работает="да">        <Serial Forward="yes">            <MultLinks Exp="*">                <A>                    <Serial Forward="yes">                        <LoadVariable VariableName="Grazb" DefaultValue='1'/>                        <Div><K>100</K></Div>                    </Serial>                </A>                <B>                    <LoadVariable VariableName="G8" DefaultValue='1'/>                </B>            </MultLinks>            <SaveVariable VariableName="G10" DefaultValue='1'/>        </Serial>   </Model>   <Model Name='G7' Работает="да">        <Serial Forward="yes">            <MultLinks Exp="*">                <A>                    <Summator Sign="+">                        <Branch Sign="+">                            <P><K>1</K></P>                        </Branch>                        <Branch Sign="-">                            <Serial Forward="yes">                                <LoadVariable VariableName="Grazb" DefaultValue='1'/>                                <Div><K>100</K></Div>                            </Serial>                                                    </Branch>                    </Summator>                                                            </A>                <B>                    <LoadVariable VariableName="G8" DefaultValue='1'/>                </B>            </MultLinks>            <SaveVariable VariableName="G7" DefaultValue='1'/>        </Serial>   </Model>   <Model Name='G4' Работает="да">        <Serial Forward="yes">            <Select Exp=">">                <A>                    <LoadVariable VariableName="SeasonSW" DefaultValue='1'/>                </A>                <B>                    <LoadVariable VariableName="Gmax" DefaultValue='10'/>                </B>                <Then>                    <P><K VariableName="a2">0</K></P>                </Then>                <Input>                    <LoadVariable VariableName="SeasonSW" DefaultValue='1'/>                </Input>            </Select>            <SaveVariable VariableName="G4" DefaultValue='1'/>        </Serial>    </Model>    <Model Name='G2' Работает="да">        <Serial Forward="yes">            <MultLinks Exp="*">                <A>                    <Serial Forward="yes">                        <LoadVariable VariableName="Gmax" DefaultValue='10'/>                        <Div><K>100</K></Div>                    </Serial>                </A>                <B>                    <LoadVariable VariableName="a2" DefaultValue='60'/>                </B>            </MultLinks>            <SaveVariable VariableName="G2" DefaultValue='1'/>        </Serial>   </Model>   <Model Name='G8s' Работает="да">        <Serial Forward="yes">            <Summator Sign="+">                <Branch Sign="+">                    <LoadVariable VariableName="G10" DefaultValue='1'/>                </Branch>                <Branch Sign="+">                                        <LoadVariable VariableName="G7" DefaultValue='1'/>                                                                             </Branch>            </Summator>            <SaveVariable VariableName="G8s" DefaultValue='1'/>        </Serial>   </Model>   <Model Name='G5' Работает="да">        <Serial Forward="yes">            <Summator Sign="+">                <Branch Sign="+">                    <LoadVariable VariableName="G4" DefaultValue='1'/>                </Branch>                <Branch Sign="+">                                        <LoadVariable VariableName="G2" DefaultValue='1'/>                                                                             </Branch>            </Summator>            <SaveVariable VariableName="G5" DefaultValue='1'/>        </Serial>   </Model>   <Model Name='TE5' Работает="да">        <Serial Forward="yes">            <MultLinks Exp="/">                <A>                    <Summator Sign="+">                        <Branch Sign="+">                            <MultLinks Exp="*">                                <A><LoadVariable VariableName="G2" DefaultValue='1'/></A>                                <B><LoadVariable VariableName="te3" DefaultValue='1'/></B>                            </MultLinks>                        </Branch>                        <Branch Sign="+">                            <MultLinks Exp="*">                                <A><LoadVariable VariableName="G4" DefaultValue='1'/></A>                                <B><LoadVariable VariableName="te4" DefaultValue='1'/></B>                            </MultLinks>                        </Branch>                    </Summator>                </A>                <B>                    <Select Exp=">">                        <A>                            <LoadVariable VariableName="G5" DefaultValue='1'/>                        </A>                        <B><P><K>1</K></P></B>                        <Then><P><K VariableName="a2">0</K></P></Then>                        <Input>                            <LoadVariable VariableName="G5" DefaultValue='1'/>                        </Input>                    </Select>                </B>            </MultLinks>            <SaveVariable VariableName="te5" DefaultValue='1'/>        </Serial>   </Model>   <Model Name='TE8' Работает="да">        <Serial Forward="yes">            <MultLinks Exp="/">                <A>                    <Summator Sign="+">                        <Branch Sign="+">                            <MultLinks Exp="*">                                <A><LoadVariable VariableName="G7" DefaultValue='1'/></A>                                <B><LoadVariable VariableName="te7" DefaultValue='0'/></B>                            </MultLinks>                        </Branch>                        <Branch Sign="+">                            <MultLinks Exp="*">                                <A><LoadVariable VariableName="G10" DefaultValue='1'/></A>                                <B><LoadVariable VariableName="te9" DefaultValue='1'/></B>                            </MultLinks>                        </Branch>                    </Summator>                </A>                <B>                                        <LoadVariable VariableName="G8s" DefaultValue='1'/>                                        </B>            </MultLinks>            <SaveVariable VariableName="te8" DefaultValue='1'/>        </Serial>   </Model>   <Model Name='TE9' Работает="да">        <Serial Forward="yes">           <Select Exp=">">                <A>                    <Serial Forward="yes">                        <Summator Sign="+">                        <Branch Sign="+">                            <LoadVariable VariableName="te10" DefaultValue='1'/>                        </Branch>                        <Branch Sign="+">                            <MultLinks Exp="/">                                <A>                                    <MultLinks Exp="*">                                        <A>                                            <MultLinks Exp="*">                                                <A><LoadVariable VariableName="G5" DefaultValue='1'/></A>                                                <B>                                                    <Summator Sign="+">                                                        <Branch Sign="+">                                                            <LoadVariable VariableName="te5" DefaultValue='1'/>                                                        </Branch>                                                        <Branch Sign="-">                                                            <LoadVariable VariableName="te11" DefaultValue='1'/>                                                        </Branch>                                                    </Summator>                                                </B>                                            </MultLinks>                                        </A>                                        <B><P><K>0.95</K></P></B>                                    </MultLinks>                                </A>                                <B>                                    <Select Exp=">">                                        <A><LoadVariable VariableName="G10" DefaultValue='1'/></A>                                        <B><P><K>1</K></P></B>                                        <Then><P><K VariableName="a2">0</K></P></Then>                                        <Input><LoadVariable VariableName="G10" DefaultValue='1'/></Input>                                    </Select>                                </B>                            </MultLinks>                        </Branch>                    </Summator>                       <AP><K>0.65</K><T>100</T></AP>                    </Serial>                                   </A>                                                        <B>                    <Serial Forward="yes">                        <LoadVariable VariableName="te10" DefaultValue='1'/>                        <AP><K>0.95</K><T>100</T></AP>                    </Serial>                </B>                <Then><P><K VariableName="a2">0</K></P></Then>                <Input>                    <Summator Sign="+">                        <Branch Sign="+">                            <LoadVariable VariableName="te5" DefaultValue='1'/>                        </Branch>                        <Branch Sign="-">                            <LoadVariable VariableName="te10" DefaultValue='1'/>                        </Branch>                    </Summator>                </Input>            </Select>            <SaveVariable VariableName="te9" DefaultValue='1'/>        </Serial>   </Model>      <Model Name='TE11' Работает="да">       <Serial Forward="yes">           <Summator Sign="+">               <Branch Sign="+">                   <LoadVariable VariableName="te5" DefaultValue='1'/>               </Branch>               <Branch Sign="-">                    <MultLinks Exp="/">                        <A>                            <MultLinks Exp="/">                                <A>                                    <MultLinks Exp="*">                                        <A><LoadVariable VariableName="G10" DefaultValue='1'/></A>                                        <B>                                            <Summator Sign="+">                                                <Branch Sign="+">                                                    <LoadVariable VariableName="te10" DefaultValue='1'/>                                                </Branch>                                                <Branch Sign="-">                                                    <LoadVariable VariableName="te9" DefaultValue='1'/>                                                </Branch>                                            </Summator>                                        </B>                                    </MultLinks>                                </A>                                <B><P><K>0.95</K></P></B>                            </MultLinks>                        </A>                        <B>                            <Select Exp=">">                                <A><LoadVariable VariableName="G5" DefaultValue='1'/></A>                                <B><P><K>1</K></P></B>                                <Then><P><K VariableName="a2">0</K></P></Then>                                <Input><LoadVariable VariableName="G5" DefaultValue='1'/></Input>                            </Select>                        </B>                    </MultLinks>               </Branch>           </Summator>           <AP><K>0.65</K><T>100</T></AP>           <SaveVariable VariableName="te11" DefaultValue='1'/>       </Serial>   </Model>   <Model Name='TE6' Работает="да">        <Serial Forward="yes">           <Select Exp=">">                <A>                    <Serial Forward="yes">                        <Summator Sign="+">                        <Branch Sign="+">                            <LoadVariable VariableName="te8" DefaultValue='1'/>                        </Branch>                        <Branch Sign="+">                            <MultLinks Exp="/">                                <A>                                    <MultLinks Exp="*">                                        <A>                                            <MultLinks Exp="*">                                                <A><LoadVariable VariableName="G2" DefaultValue='1'/></A>                                                <B>                                                    <Summator Sign="+">                                                        <Branch Sign="+">                                                            <LoadVariable VariableName="te2" DefaultValue='1'/>                                                        </Branch>                                                        <Branch Sign="-">                                                            <LoadVariable VariableName="te3" DefaultValue='1'/>                                                        </Branch>                                                    </Summator>                                                </B>                                            </MultLinks>                                        </A>                                        <B><P><K>0.95</K></P></B>                                    </MultLinks>                                </A>                                <B><LoadVariable VariableName="G8" DefaultValue='1'/>                                </B>                            </MultLinks>                        </Branch>                    </Summator>                       <AP><K>0.65</K><T>100</T></AP>                    </Serial>                                   </A>                                                        <B>                    <Serial Forward="yes">                        <LoadVariable VariableName="te8" DefaultValue='1'/>                        <AP><K>0.95</K><T>100</T></AP>                    </Serial>                </B>                <Then><P><K VariableName="a2">0</K></P></Then>                <Input>                    <Summator Sign="+">                        <Branch Sign="+">                            <LoadVariable VariableName="te2" DefaultValue='1'/>                        </Branch>                        <Branch Sign="-">                            <LoadVariable VariableName="te8" DefaultValue='1'/>                        </Branch>                    </Summator>                </Input>            </Select>            <SaveVariable VariableName="te6" DefaultValue='1'/>        </Serial>   </Model>      <Model Name='TE3' Работает="да">       <Serial Forward="yes">           <Summator Sign="+">               <Branch Sign="-">                   <LoadVariable VariableName="te5" DefaultValue='1'/>               </Branch>               <Branch Sign="+">                    <MultLinks Exp="/">                        <A>                            <MultLinks Exp="/">                                <A>                                    <MultLinks Exp="*">                                        <A><LoadVariable VariableName="G8" DefaultValue='1'/></A>                                        <B>                                            <Summator Sign="+">                                                <Branch Sign="+">                                                    <LoadVariable VariableName="te6" DefaultValue='1'/>                                                </Branch>                                                <Branch Sign="-">                                                    <LoadVariable VariableName="te8" DefaultValue='1'/>                                                </Branch>                                            </Summator>                                        </B>                                    </MultLinks>                                </A>                                <B><P><K>0.95</K></P></B>                            </MultLinks>                        </A>                        <B>                            <Select Exp=">">                                <A><LoadVariable VariableName="G2" DefaultValue='1'/></A>                                <B><P><K>1</K></P></B>                                <Then><P><K VariableName="a2">0</K></P></Then>                                <Input><LoadVariable VariableName="G2" DefaultValue='1'/></Input>                            </Select>                        </B>                    </MultLinks>               </Branch>           </Summator>           <AP><K>0.65</K><T>100</T></AP>           <SaveVariable VariableName="te3" DefaultValue='1'/>       </Serial>   </Model>   <Model Name='TE7' Работает="да">       <Serial Forward="yes">           <LoadVariable VariableName="te6" DefaultValue='1'/>           <AP><K>0.9375</K><T>10</T></AP>           <SaveVariable VariableName="te7" DefaultValue='1'/>       </Serial>   </Model>      <Model Name='переключатель' Работает="да">        <Serial Forward="yes">            	            <LoadVariable VariableName="dp1" DefaultValue='0'/>            <SaveVariable VariableName="dp1" DefaultValue='0'/>	</Serial>        </Model>   </Schema>