<?xml version='1.0' encoding='UTF-8'?><Schema>    <Model Name='Подготовка начальных условий' Работает="да">        <Serial Forward="yes">            <CreateVariable VariableName="te2" DefaultValue='95'/>            <CreateVariable VariableName="te10" DefaultValue='9'/>            <CreateVariable VariableName="te4" DefaultValue='70'/>            <CreateVariable VariableName="Grazb" DefaultValue='0'/>	            <CreateVariable VariableName="Gmax" DefaultValue='10'/>            <CreateVariable VariableName="G8divGmax" DefaultValue='0.5'/>            <CreateVariable VariableName="SeasonSW" DefaultValue='1'/>	            <CreateVariable VariableName="a2" DefaultValue='60'/>			            <CreateVariable VariableName="G8" DefaultValue='0'/>            <CreateVariable VariableName="G4" DefaultValue='0'/>            <CreateVariable VariableName="G2" DefaultValue='0'/>            <CreateVariable VariableName="G10" DefaultValue='0'/>            <CreateVariable VariableName="G7" DefaultValue='0'/>            <CreateVariable VariableName="G8s" DefaultValue='0'/>            <CreateVariable VariableName="G5" DefaultValue='0'/>            <CreateVariable VariableName="te8" DefaultValue='0'/>            <CreateVariable VariableName="te5" DefaultValue='0'/>            <CreateVariable VariableName="te6" DefaultValue='0'/>            <CreateVariable VariableName="te3" DefaultValue='0'/>            <CreateVariable VariableName="te9" DefaultValue='0'/>            <CreateVariable VariableName="te11" DefaultValue='0'/>            <CreateVariable VariableName="te7" DefaultValue='0'/>	    </Serial>    </Model>	<Model Name='Нормирование сигнала о положении клапана на каждом цыкле' Работает="да">        <Serial Forward="yes">			<LoadVariable VariableName="a2" DefaultValue='0'/>				<Div><K>100</K></Div>			<SaveVariable VariableName="a2" DefaultValue='0'/>		    </Serial>    </Model>    <Model Name='G8' Работает="да">        <Serial Forward="yes">            <MultLinks Exp = "*">                <A><LoadVariable VariableName="Gmax" DefaultValue ='0'/></A>                <B><LoadVariable VariableName="G8divGmax" DefaultValue ='0.5'/></B>            </MultLinks>            <SaveVariable VariableName="G8" DefaultValue='0'/>	</Serial>    </Model>    <Model Name='G10' Работает="да">        <Serial Forward="yes">            <MultLinks Exp="*">                <A>                    <Serial Forvard="yes">                        <LoadVariable VariableName="Grazb" DefaultValue='0'/>                        <Div><K>100</K></Div>                    </Serial>                </A>                <B>                    <LoadVariable VariableName="G8" DefaultValue='0'/>                </B>            </MultLinks>            <SaveVariable VariableName="G10" DefaultValue='0'/>        </Serial>   </Model>   <Model Name='G7' Работает="да">        <Serial Forward="yes">            <MultLinks Exp="*">                <A>                    <Summator>                        <Branch Sign="+">                            <P><K>1</K></P>                        </Branch>                        <Branch Sign="-">                            <Serial Forward="yes">                                <LoadVariable VariableName="Grazb" DefaultValue='0'/>                                <Div><K>100</K></Div>                            </Serial>                                                    </Branch>                    </Summator>                                                            </A>                <B>                    <LoadVariable VariableName="G8" DefaultValue='0'/>                </B>            </MultLinks>            <SaveVariable VariableName="G7" DefaultValue='0'/>        </Serial>   </Model>   <Model Name='G4' Работает="да">        <Serial Forward="yes">            <Select Exp=">">                <A>                    <LoadVariable VariableName="SeasonSW" DefaultValue='1'/>                </A>                <B>                    <LoadVariable VariableName="Gmax" DefaultValue='10'/>                </B>                <Then>                    <P><K>0</K></P>                </Then>            </Select>            <SaveVariable VariableName="G4" DefaultValue='0'/>        </Serial>    </Model>    <Model Name='G2' Работает="да">        <Serial Forward="yes">            <MultLinks Exp="*">                <A>                    <Serial Forvard="yes">                        <LoadVariable VariableName="Gmax" DefaultValue='10'/>                        <Div><K>100</K></Div>                    </Serial>                </A>                <B>                    <LoadVariable VariableName="a2" DefaultValue='0'/>                </B>            </MultLinks>            <SaveVariable VariableName="G2" DefaultValue='0'/>        </Serial>   </Model></Schema>